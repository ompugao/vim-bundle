snippet     footnote
options     word
    \footnote${1:#:[num(optional)]}{${2:#:text}}${0}

snippet     includegraphics
options     word
    \includegraphics[${1:width=}]{${2}}

snippet     subfigure
options     word
    \subfigure[${1:#:caption}]{
        \includegraphics[${2:#:width=}]{${3}}
    }
